<fileSetConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Statistics about a reads file</name>
    <id>READS_STATS_PROPERTIES_FILE</id>
    <help>Statistics about a reads file, in Java properties format. Contains file size information and other meta-data. </help>
    <version>1.0</version>
    <mimeType>text/plain</mimeType>
    <files>
        <selector>
            <id>STATS</id>
            <pattern>*.properties</pattern>
            <mandatory>true</mandatory>
        </selector>
    </files>
</fileSetConfig>