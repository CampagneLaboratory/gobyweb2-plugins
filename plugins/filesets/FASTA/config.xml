<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<fileSetConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Fasta-Reads</name>
    <id>FASTA</id>
    <help>fasta reads format</help>
    <version>1.0</version>
    <mimeType>application/fasta-reads</mimeType>
    <files>
        <selector matchAttribute="basename">
            <id>READS_FILE</id>
            <pattern>*.fa</pattern>
            <mandatory>true</mandatory>
        </selector>
    </files>
    <attributes>
        <attribute name="basename"/>
    </attributes>
</fileSetConfig>
