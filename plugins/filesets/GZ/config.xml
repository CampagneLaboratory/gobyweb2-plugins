<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<fileSetConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>GZ Compressed Archive</name>
    <id>GZ</id>
    <help>GZ Compressed Archive</help>
    <version>1.0</version>
    <mimeType>application/x-gzip-compressed</mimeType>
    <files>
        <selector matchAttribute="BASENAME">
            <id>GZ</id>
            <pattern>*.gz</pattern>
            <mandatory>true</mandatory>
        </selector>
    </files>
    <attributes>
        <attribute name="BASENAME"/>
    </attributes>
</fileSetConfig>