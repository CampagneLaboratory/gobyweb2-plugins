<resourceConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Pathogen data</name>
    <id>PATHOGEN_DATA</id>
    <help>Pathogen data with viral and bacterial nucleotide sequences.
    </help>
    <version>1.1</version>
    <requires>
        <resource>
            <id>FETCH_URL</id>
            <versionAtLeast>1.2</versionAtLeast>
        </resource>
        <resource>
            <id>LAST_ARTIFACT</id>
            <versionAtLeast>581</versionAtLeast>
        </resource>
    </requires>
    <artifacts>
        <artifact id="FUNGI" order="1"/>
        <artifact id="MICROBIAL" order="2"/>
        <artifact id="VIRAL" order="3"/>
    </artifacts>
</resourceConfig>