<resourceConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Process Reads Scripts</name>
    <id>PROCESS_READS</id>
    <help>The scripts that process the read files uploaded to GobyWeb.</help>
    <version>1.1</version>
    <requires>
        <resource>
            <id>GROOVY</id>
            <versionAtLeast>2.0.6</versionAtLeast>
        </resource>
        <resource>
            <id>FETCH_URL</id>
            <versionAtLeast>1.2</versionAtLeast>
        </resource>
        <resource>
            <id>GOBY3</id>
            <versionExactly>3.3.0</versionExactly>
        </resource>
        <resource>
            <id>JAVA</id>
            <versionAtLeast>1.8</versionAtLeast>
        </resource>
    </requires>
    <files>
        <file>
            <id>PROCESS_SAMPLES</id>
            <filename>TaskProcessSamples.groovy</filename>
        </file>

    </files>
</resourceConfig>
