<resourceConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Sample resource created with MPS</name>
    <id>SAMPLE_RESOURCE_WITH_MPS</id>
    <help>Sample resource created with MPS.</help>
    <version>1.0</version>
    <requires>
        <resource>
            <id>GOBYWEB_SERVER_SIDE</id>
            <versionAtLeast>1.1</versionAtLeast>
        </resource>
        <resource>
            <id>MPS</id>
            <versionAtLeast>129.350</versionAtLeast>
        </resource>
    </requires>
    <artifacts>
        <artifact id="FIRST_ARTIFACT"></artifact>
        <artifact id="SECOND_ARTIFACT"></artifact>
    </artifacts>
    <files>
        <file>
            <id>RUN_MODEL_SCRIPT</id>
            <filename>run_model.sh</filename>
        </file>
        <file>
            <id>PLUGIN_CLASSES</id>
            <filename>DeployableGobyWebPlugins</filename>
            <isDirectory>true</isDirectory>
        </file>
    </files>
</resourceConfig>

