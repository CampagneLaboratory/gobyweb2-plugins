<resourceConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>EdgeR script</name>
    <id>EDGE_R_SCRIPT</id>
    <help>Script that helps run EdgeR on table of counts data with group information.</help>
    <version>2013.05.17</version>
    <requires>
        <resource>
            <id>EDGER</id>
            <versionAtLeast>2.6.12.1</versionAtLeast>
        </resource>
