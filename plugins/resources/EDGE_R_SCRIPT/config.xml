    <files>
        <file>
            <id>R_SCRIPT</id>
            <filename>deAnalysisEdgeR.R</filename>
        </file>
        <file>
            <id>SETUP</id>
            <filename>edgeR-setup.sh</filename>
        </file>
    </files>
</resourceConfig>
