<resourceConfig xsi:noNamespaceSchemaLocation="../../../schemas/plugins.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>MuTect</name>
    <id>MUTECT</id>
    <help>MuTect is a method developed at the Broad Institute for the reliable and accurate
        identification of somatic point mutations in next generation sequencing data of cancer genomes.
    </help>
    <version>1.1.4</version>
    <artifacts>
        <artifact id="BINARIES" order="1"/>
    </artifacts>
    <files>
        <file>
            <id>EXEC_PATH</id>
            <filename>mutect</filename>
        </file>
        <file>
            <id>UNZIP_EXEC_PATH</id>
            <filename>unzip</filename>
        </file>
    </files>
</resourceConfig>
